{
  "name": "@p4in1/virtual-grid",
  "version": "0.0.47",
  "description": "This is a grid component that is extremely fast and can manage up to 500k rows",
  "main": "dist/virtual.grid.min.js",
  "module": "dist/virtual.grid.min.js",
  "types": "dist/virtual.grid.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "mkDir": "if exist dist rd /s /q dist && mkdir dist && if exist tmp rd /s /q tmp && mkdir tmp",
    "clean": "if exist declarations rd /s /q declarations",
    "assets": "mkdir \"dist/assets\" && copy \"my-grid/src/virtual-grid/styles/assets\" \"dist/assets\"",
    "build": "npm run mkDir && rollup -c && npm run clean && npm run assets"
  },
  "keywords": [
    "grid",
    "fast",
    "performance",
    "free",
    "virtual",
    "infinite",
    "scroll",
    "definition",
    "column",
    "row"
  ],
  "author": "P4in1",
  "license": "MIT",
  "devDependencies": {
    "rollup": "^2.28.1",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-scss": "^2.6.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/p4in1/virtual-grid.git"
  },
  "bugs": {
    "url": "https://github.com/p4in1/virtual-grid/issues"
  },
  "homepage": "http://virtual-grid.com"
}
